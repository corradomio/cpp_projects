#ifndef BOHEM_SET_H
#define BOHEM_SET_H

#include <set>
#include "allocator"

namespace bohem {

    template<typename _Key, typename _Compare = std::less<_Key>>
    using set = std::set<_Key, _Compare, bohem::allocator<_Key>>;

}

#endif //BOHEM_SET_H
