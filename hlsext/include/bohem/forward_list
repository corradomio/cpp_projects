#ifndef BOHEM_FORWARD_LIST_H
#define BOHEM_FORWARD_LIST_H

#include <forward_list>
#include "allocator.h"

namespace bohem {

    template<typename _Tp>
    using forward_list = std::forward_list<_Tp, bohem::allocator<_Tp>>;
}

#endif //BOHEM_FORWARD_LIST_H
