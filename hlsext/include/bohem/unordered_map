#ifndef BOHEM_UNORDERED_MAP_H
#define BOHEM_UNORDERED_MAP_H

#include <unordered_map>
#include "allocator.h"

namespace bohem {

    template<typename _Key, typename _Tp,
        typename _Hash = std::hash<_Key>,
        typename _Pred = std::equal_to<_Key>>
    using unordered_map = std::unordered_map<_Key, _Tp, _Hash, _Pred, bohem::allocator<std::pair<const _Key, _Tp>> >;

}

#endif //BOHEM_UNORDERED_MAP_H
