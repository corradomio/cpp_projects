#ifndef BOHEM_UNORDERED_SET_H
#define BOHEM_UNORDERED_SET_H

#include <unordered_set>
#include "allocator"

namespace bohem {

    template<typename _Value,
            typename _Hash = std::hash<_Value>,
            typename _Pred = std::equal_to<_Value>>
    using unordered_set = std::unordered_set<_Value, _Hash, _Pred, bohem::allocator<_Value>>;

}

#endif //BOHEM_UNORDERED_SET_H
