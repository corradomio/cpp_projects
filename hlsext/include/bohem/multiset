#ifndef BOHEM_MULTISET_H
#define BOHEM_MULTISET_H

#include <set>
#include <multiset>
#include "allocator.h"

namespace bohem {

    template<typename _Key, typename _Compare = std::less<_Key>>
    using multiset = std::multiset<_Key, _Compare, bohem::allocator<_Key>>;

}

#endif //BOHEM_MULTISET_H
