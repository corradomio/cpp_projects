#ifndef BOHEM_MULTIMAP_H
#define BOHEM_MULTIMAP_H

#include <multimap>
#include "allocator"

namespace bohem {

    template<typename _Key, typename _Tp, typename _Compare = std::less<_Key>>
    using multimap = std::multimap<_Tp, bohem::allocator<_Tp>, bohem::allocator<std::pair<const _Key, _Tp>> >;

}

#endif //BOHEM_MULTIMAP_H
